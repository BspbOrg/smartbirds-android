<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.bspb.smartbirds.pro">

    <uses-permission android:name="android.permission.ACCESS_MOCK_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- requestLegacyExternalStorage:true for writing screenshots to public storage on sdk 29 (Q) -->
    <!-- preserveLegacyExternalStorage:true for writing screenshots to public storage on sdk 30 (Q) -->
    <application android:requestLegacyExternalStorage="true" android:preserveLegacyExternalStorage="true" />
</manifest>
